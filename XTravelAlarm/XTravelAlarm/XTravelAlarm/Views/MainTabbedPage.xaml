<?xml version="1.0" encoding="utf-8"?>

<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            x:Class="XTravelAlarm.Views.MainTabbedPage"
            xmlns:prism="clr-namespace:Prism.Mvvm;assembly=Prism.Forms"
            prism:ViewModelLocator.AutowireViewModel="True"
            xmlns:main="clr-namespace:XTravelAlarm.Views.Main;assembly=XTravelAlarm"
            xmlns:alarmPage="clr-namespace:XTravelAlarm.Views.AlarmPage;assembly=XTravelAlarm"
            Title="XTravel Alarm">
    <!--Pages can be added as references or inline-->

    
    <main:MainPage>
        <main:MainPage.Icon>
            <OnPlatform x:TypeArguments="FileImageSource"
                        Android=""
                        iOS="ic_alarm_add.png" />
        </main:MainPage.Icon>
       
    </main:MainPage>


    <alarmPage:AlarmsPage>
        <alarmPage:AlarmsPage.Icon>
            <OnPlatform x:TypeArguments="FileImageSource"
                        Android=""
                        iOS="ic_alarm.png" />
        </alarmPage:AlarmsPage.Icon>
    </alarmPage:AlarmsPage>


</TabbedPage>